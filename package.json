{
  "name": "constitutional-handshake-protocol",
  "version": "1.0.0",
  "description": "A decentralized protocol for constitutional AI agents to recognize and trust each other",
  "type": "module",
  "main": "./dist/chp.cjs",
  "module": "./dist/chp.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/chp.js",
      "require": "./dist/chp.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p config/tsconfig.json && vite build --config config/vite.config.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext ts -c config/.eslintrc.json --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,js,md}\"",
    "type-check": "tsc -p config/tsconfig.json --noEmit",
    "demo": "node examples/simple-demo.cjs",
    "clean": "rimraf dist"
  },
  "keywords": [
    "constitutional-ai",
    "handshake-protocol",
    "trust-graph",
    "behavioral-attestation",
    "decentralized",
    "cryptography",
    "ai-governance",
    "constitutional-identity"
  ],
  "author": "Constitutional AI Research",
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/krpofficial/Paulsens-Freedom-Preserving-Five.git"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
